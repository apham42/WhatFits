      <!--IM NOT SURE IF THIS IS RIGHT -Aaron-->
      <add name="Content-Security-Policy" value="default-src 'self';"/>
    </customHeaders>
  </httpProtocol>
    <handlers>
      <remove name="ExtensionlessUrlHandler-Integrated-4.0" />
      <!--IM NOT SURE IF THIS IS RIGHT -Aaron-->
      <!--<remove name="OPTIONSVerbHandler" />-->
