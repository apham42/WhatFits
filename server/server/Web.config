  <system.web>
    <compilation debug="true" targetFramework="4.6.1" />
    <httpRuntime targetFramework="4.6.1" />
  </system.web>
  <!--User Access Control-->
  <system.identityModel>
    <identityConfiguration>
      <claimsAuthenticationManager type="Whatfits.UserAccessControl.Auth.ClaimsTransformer, Whatfits.UserAccessControl" />
      <claimsAuthorizationManager type="Whatfits.UserAccessControl.Auth.AuthorizationManager, Whatfits.UserAccessControl" />
